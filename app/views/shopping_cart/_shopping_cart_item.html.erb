<% product = Product.find(shopping_cart_item.item_id) %>
<tr>
  <td><%= shopping_cart_item.id %></td>
  <td><%= product.title %></td>
  <td><%= shopping_cart_item.price.to_i.floor   %></td>
  <td><%= shopping_cart_item.quantity %></td>
  <td><%= link_to 'Delete', shopping_cart_path(product), method: :delete, data: { confirm: 'Are you sure to delete?'} %></td>
</tr>